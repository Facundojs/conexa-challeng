{
  "version": 2,
  "buildCommand": "npx @vercel/ncc build src/main.ts --out dist/main.js",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/ncc"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST"
      ]
    }
  ]
}